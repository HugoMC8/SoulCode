programa {
	funcao inicio() {
		inteiro senha
		
		faca{
		    escreva("Digite sua senha")
		    leia(senha)
		    se(senha == 1234){
		        escreva("Senha correta \n")
		    }senao{
		        escreva("Senha Incorreta \n")
		    }
		}enquanto(senha != 1234)
		
		escreva("Bem vindo ao sistema")
	}
}
